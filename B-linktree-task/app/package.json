{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "NODE_ENV=production node dist/app.js",
    "build": "babel src -d dist --source-maps",
    "start": "NODE_ENV=development babel-node src/app.js",
    "dev": "DEBUG=server:debug NODE_ENV=development nodemon src/app.js",
    "test": "DEBUG=server:test NODE_ENV=test nyc --reporter=text mocha --require babel-core/register --reporter spec --exit   tests/ --exec babel-node --debug",
    "test:watch": "DEBUG=server:debug NODE_ENV=development mocha  --require babel-core/register --watch --reporter  spec   tests/ --exec babel-node",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "babel-cli": "^6.26.0",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "config": "^3.3.1",
    "debug": "^4.1.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "node/no-unsupported-features/es-syntax": 0,
      "node/no-unsupported-features/es-builtins": 0
    },
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  }
}
